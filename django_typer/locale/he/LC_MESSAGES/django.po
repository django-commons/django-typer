#~ msgid "Unable to import fallback completion function: {err}"
#~ msgstr "לא ניתן לייבא את פונקציית השלמת החזרה: {err}"

#~ msgid ""
#~ "Please specify the shell to install or remove autocompletion for. Unable to "
#~ "detect shell."
#~ msgstr ""
#~ "נא לציין את המעטפת להתקנה או להסיר השלמה אוטומטית עבורה. לא ניתן לזהות "
#~ "מעטפת."

#, python-brace-format
#~ msgid "Unsupported shell: {shell}"
#~ msgstr "מעטפת לא נתמכת: {shell}"

#, python-brace-format
#~ msgid ""
#~ "Shell {shell} does not support autocompletion for scripts that are not "
#~ "installed on the path. You must create an entry point for {script_name}. See"
#~ " {link}."
#~ msgstr ""
#~ "מעטפת {shell} אינה תומכת בהשלמה אוטומטית עבור סקריפטים שאינם מותקנים על "
#~ "הנתיב. עליך ליצור נקודת כניסה עבור {script_name}. ראה {קישור}."

#, python-brace-format
#~ msgid ""
#~ "It is not recommended to install tab completion for a script not on the path"
#~ " because completions will likely only work if the script is invoked from the"
#~ " same location and using the same relative path. You may wish to create an "
#~ "entry point for {script_name}. See {link}."
#~ msgstr ""
#~ "לא מומלץ להתקין השלמת כרטיסיות עבור סקריפט שאינו נמצא בנתיב מכיוון שהשלמות "
#~ "יפעלו רק אם הסקריפט מופעל מאותו מיקום ומשתמש באותו נתיב יחסי. ייתכן שתרצה "
#~ "ליצור נקודת כניסה עבור {script_name}. ראה {קישור}."

#, python-brace-format
#~ msgid "Installed autocompletion for {shell} @ {install_path}"
#~ msgstr "השלמה אוטומטית מותקנת עבור {shell} @ {install_path}"

#, python-brace-format
#~ msgid "Uninstalled autocompletion for {shell}."
#~ msgstr "השלמה אוטומטית שהוסרה עבור {shell}."
